<ul class="task-list">
    {% for contract in user.contract %}
    <li class="info">
        <a href="{{ url('contract_edit' , {'id':contract.id}) }}" class="task-close"><i class="fa fa-edit"></i></a>
        <div class="row">
        <div class="col-md-6">

            <span class="{{ contract.contractStatus.id | contract_status_color }}">{{ contract.contractStatus.id | contract_status }}</span>

            <label class="checkbox-inline">
                <a href="{{ url('contract_edit' , {'id':contract.id}) }}">{{ contract.projectName }}</a>
            </label>

        </div>

        <div class="col-md-6">
            {{ contract.contractNo }}
        </div>

    </li>
    {% endfor %}
</ul>